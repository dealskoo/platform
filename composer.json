{
    "name": "dealskoo/platform",
    "description": "Dealskoo Platform",
    "homepage": "https://www.dealskoo.com",
    "support": {
        "issues": "https://github.com/dealskoo/platform/issues",
        "source": "https://github.com/dealskoo/platform"
    },
    "keywords": [
        "Laravel",
        "Dealskoo Platform",
        "Dealskoo"
    ],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Dealskoo\\Platform\\": "src/",
            "Database\\Factories\\Dealskoo\\Platform\\Models\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dealskoo\\Platform\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "hadi",
            "email": "hadi@dealskoo.com",
            "homepage": "https://www.dealskoo.com"
        }
    ],
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    },
    "require": {
        "php": ">=7.1",
        "dealskoo/admin": "dev-master",
        "dealskoo/country": "dev-master",
        "dealskoo/seller": "dev-master",
        "laravel/framework": ">=7.20"
    },
    "require-dev": {
        "orchestra/testbench": "^6.23",
        "orchestra/canvas": "^6.10",
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dealskoo\\Platform\\Providers\\PlatformServiceProvider"
            ],
            "aliases": {
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
